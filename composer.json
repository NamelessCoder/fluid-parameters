{
    "name": "namelesscoder/fluid-parameters",
    "description": "API to declare parameters for any Fluid template/section",
    "type": "typo3-cms-extension",
    "autoload": {
        "psr-4": {
            "NamelessCoder\\FluidParameters\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NamelessCoder\\FluidParameters\\Tests\\": "Tests/"
        }
    },
    "require": {
        "typo3fluid/fluid": "^2"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "fluid_parameters"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9 || ^10.0",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "^3.7",
        "php-coveralls/php-coveralls": "^2.5"
    }
}
